<Application
    x:Class="WinUI3Examples.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:cvrt="using:WinUI3Examples.Converters"
    xmlns:local="using:WinUI3Examples">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <ResourceDictionary Source="ResourceDictionaries\ControlStyles.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries\ThemeDictionaries.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <cvrt:NullToVisibilityConverter x:Key="NullToVisibilityConverter"/>

        </ResourceDictionary>
    </Application.Resources>
</Application>
